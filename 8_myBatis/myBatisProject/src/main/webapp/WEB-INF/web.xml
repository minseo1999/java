<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" version="6.0">
  <servlet>
    <description></description>
    <display-name>login.me</display-name>
    <servlet-name>login.me</servlet-name>
    <servlet-class>com.kh.mybatis.member.controller.memberLoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login.me</servlet-name>
    <url-pattern>/login.me</url-pattern>
  </servlet-mapping>
  <display-name>myBatisProject</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>enrollForm.me</display-name>
    <servlet-name>enrollForm.me</servlet-name>
    <servlet-class>com.kh.mybatis.member.controller.MemberEnrollController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>enrollForm.me</servlet-name>
    <url-pattern>/enrollForm.me</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>insert.me</display-name>
    <servlet-name>insert.me</servlet-name>
    <servlet-class>com.kh.mybatis.member.controller.MemberInsertController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>insert.me</servlet-name>
    <url-pattern>/insert.me</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>list.bo</display-name>
    <servlet-name>list.bo</servlet-name>
    <servlet-class>com.kh.mybatis.board.controller.BoardListController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>list.bo</servlet-name>
    <url-pattern>/list.bo</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>detail.bo</display-name>
    <servlet-name>detail.bo</servlet-name>
    <servlet-class>com.kh.mybatis.board.controller.BoardDetailController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>detail.bo</servlet-name>
    <url-pattern>/detail.bo</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>search.bo</display-name>
    <servlet-name>search.bo</servlet-name>
    <servlet-class>com.kh.mybatis.board.controller.BoardSearchController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>search.bo</servlet-name>
    <url-pattern>/search.bo</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>EncodingFilter</display-name>
    <filter-name>EncodingFilter</filter-name>
    <filter-class>com.kh.mybatis.common.filter.EncodingFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>EncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <display-name>LoginFilter</display-name>
    <filter-name>LoginFilter</filter-name>
    <filter-class>com.kh.mybatis.common.filter.LoginFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>LoginFilter</filter-name>
    <url-pattern>/list.bo</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>LoginFilter</filter-name>
    <url-pattern>/detail.bo</url-pattern>
  </filter-mapping>
</web-app>